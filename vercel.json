{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/public/dist/script.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}